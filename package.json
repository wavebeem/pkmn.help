{
  "private": true,
  "version": "0.0.0",
  "name": "pkmn.help",
  "type": "module",
  "description": "The Pok√©mon Type Calculator (https://pkmn.help)",
  "scripts": {
    "update-pokedex": "ts-node --esm ./bin/update-pokedex.ts",
    "scrape-bulba": "ts-node --esm ./bin/scrape-bulba.ts",
    "language-export": "ts-node --esm ./bin/language-export.ts",
    "language-import": "ts-node --esm ./bin/language-import.ts",
    "i18n-scrape": "i18next",
    "upgrade": "npm-check -u",
    "start": "vite",
    "prebuild": "rimraf dist",
    "typecheck": "tsc -p .",
    "build": "tsc && vite build --mode production",
    "test-sw": "npm run -s build && npx serve -s dist",
    "preview": "vite preview",
    "lint:js": "eslint src -f unix --ext .ts,.tsx,.js",
    "lint:css": "stylelint -f unix \"src/**/*.css\"",
    "format": "prettier \"src/**/*.{ts,tsx,js}\" --check",
    "format:fix": "prettier \"src/**/*.{ts,tsx,js}\" --write",
    "test": "npm-run-all -s lint:js lint:css format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wavebeem/pkmn.help.git"
  },
  "keywords": [],
  "author": "Sage Fennel <sage@wavebeem.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wavebeem/pkmn.help/issues"
  },
  "homepage": "https://github.com/wavebeem/pkmn.help#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@types/jsdom": "^16.2.13",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^7.1.7",
    "i18next-parser": "^5.4.0",
    "jsdom": "^16.6.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "node-fetch": "^2.6.1",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-pwa": "^0.19.0"
  },
  "dependencies": {
    "@types/classnames": "^2.3.1",
    "@types/match-sorter": "^4.0.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "colord": "^2.9.2",
    "fastest-levenshtein": "^1.0.12",
    "i18next": "^23.9.0",
    "i18next-http-backend": "^2.4.3",
    "match-sorter": "^4.1.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.0.5",
