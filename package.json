    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^7.1.7",
    "i18next-parser": "^5.4.0",
    "jsdom": "^16.6.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
