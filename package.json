{
  "private": true,
  "version": "0.0.0",
  "name": "pkmn.help",
  "description": "Pok√©mon type calculator",
  "scripts": {
    "merge-data": "ts-node ./bin/merge-data.ts",
    "scrape-bulba": "ts-node ./bin/scrape-bulba.ts",
    "scrape-pokeapi": "ts-node ./bin/scrape-pokeapi.ts",
    "scrape-pokeapi:local": "API=http://localhost:8000/api/v2/ npm run -s scrape-pokeapi",
    "language-export": "ts-node ./bin/language-export.ts",
    "language-import": "ts-node ./bin/language-import.ts",
    "i18n-scrape": "i18next",
    "fetch-images": "ts-node ./bin/fetch-images.ts",
    "upgrade": "npm-check -u",
    "start": "vite",
    "prebuild": "rimraf dist",
    "typecheck": "tsc -p .",
    "build": "tsc && vite build --mode production",
    "test-sw": "npm run -s build && npx serve -s dist",
    "preview": "vite preview",
    "lint:js": "eslint src -f unix --ext .ts,.tsx,.js",
    "lint:css": "stylelint -f unix \"src/**/*.css\"",
    "format": "prettier \"src/**/*.{ts,tsx,js}\" --check",
    "format:fix": "prettier \"src/**/*.{ts,tsx,js}\" --write",
    "test": "npm-run-all -s lint:js lint:css format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wavebeem/pkmn.help.git"
  },
